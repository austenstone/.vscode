{
	"servers": {
		"time": {
			"command": "uvx",
			"args": [
				"mcp-server-time",
				"--local-timezone",
				"America/New_York"
			],
			"type": "stdio"
		},
		"context7": {
			"command": "npx",
			"args": [
				"-y",
				"@upstash/context7-mcp"
			],
			"type": "stdio"
		},
		"azure": {
			"command": "npx",
			"args": [
				"-y",
				"@azure/mcp@latest",
				"server",
				"start"
			],
			"type": "stdio"
		},
		"playwright": {
			"command": "npx",
			"args": [
				"@playwright/mcp@latest"
			],
			"type": "stdio"
		},
		"sequentialthinking": {
			"command": "npx",
			"args": [
				"-y",
				"@modelcontextprotocol/server-sequential-thinking"
			],
			"type": "stdio"
		},
		"github": {
			"url": "https://api.githubcopilot.com/mcp/",
			"type": "http"
		},
		"Chrome MCP Server": {
			"url": "http://127.0.0.1:12306/mcp",
			"type": "http"
		}
	},
	"inputs": [
		{
			"type": "promptString",
			"id": "zapier_mcp_url",
			"description": "Zapier MCP URL",
			"password": true
		}
	]
}